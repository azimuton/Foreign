                    android:id="@+id/tvPotato"
                    style="@style/translate"
                    android:text="Картофель" />

                <ImageView
                    android:id="@+id/ivPotato"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/radish"
                    style="@style/english"
                    android:text="Radish" />

                <TextView
                    android:id="@+id/tvRadish"
                    style="@style/translate"
                    android:text="Редис" />

                <ImageView
                    android:id="@+id/ivRadish"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/pepper"
                    style="@style/english"
                    android:text="Pepper" />

                <TextView
                    android:id="@+id/tvPepper"
                    style="@style/translate"
                    android:text="Перец" />

                <ImageView
                    android:id="@+id/ivPepper"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout

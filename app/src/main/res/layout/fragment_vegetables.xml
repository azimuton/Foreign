                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/parsnip"
                    style="@style/english"
                    android:text="Parsnip" />

                <TextView
                    android:id="@+id/tvParsnip"
                    style="@style/translate"
                    android:text="Пастернак" />

                <ImageView
                    android:id="@+id/ivParsnip"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/squash"
                    style="@style/english"
                    android:text="Squash" />

                <TextView
                    android:id="@+id/tvSquash"
                    style="@style/translate"
                    android:text="Патиссон" />

                <ImageView
                    android:id="@+id/ivSquash"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>
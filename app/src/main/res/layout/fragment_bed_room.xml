<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/yellowfonone">

    <ImageView
        android:id="@+id/ivBackToNewWordsList"
        android:layout_width="78dp"
        android:layout_height="42dp"
        android:src="@drawable/leftarrow"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ScrollView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ivBackToNewWordsList"
        tools:ignore="SpeakableTextPresentCheck">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="8dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/bedroom"
                    style="@style/english"
                    android:text="Bedroom" />

                <TextView
                    android:id="@+id/tvBedroom"
                    style="@style/translate"
                    android:text="Спальня" />

                <ImageView
                    android:id="@+id/ivBedroom"
                    style="@style/image_to_main"
                    android:layout_height="match_parent"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/bed"
                    style="@style/english"
                    android:text="Bed" />

                <TextView
                    android:id="@+id/tvBed"
                    style="@style/translate"
                    android:text="Кровать" />

                <ImageView
                    android:id="@+id/ivBed"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/nightstand"
                    style="@style/english"
                    android:text="Nightstand" />

                <TextView
                    android:id="@+id/tvNightstand"
                    style="@style/translate"
                    android:text="Прикроватная тумба" />

                <ImageView
                    android:id="@+id/ivNightstand"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/closet"
                    style="@style/english"
                    android:text="Closet" />

                <TextView
                    android:id="@+id/tvCloset"
                    style="@style/translate"
                    android:text="Шкаф" />

                <ImageView
                    android:id="@+id/ivCloset"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/sconce"
                    style="@style/english"
                    android:text="Sconce" />

                <TextView
                    android:id="@+id/tvSconce"
                    style="@style/translate"
                    android:text="Бра" />

                <ImageView
                    android:id="@+id/ivSconce"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/cover"
                    style="@style/english"
                    android:text="Cover" />

                <TextView
                    android:id="@+id/tvCover"
                    style="@style/translate"
                    android:text="Покрывало" />

                <ImageView
                    android:id="@+id/ivCover"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/pillow"
                    style="@style/english"
                    android:text="Pillow" />

                <TextView
                    android:id="@+id/tvPillow"
                    style="@style/translate"
                    android:text="Подушка" />

                <ImageView
                    android:id="@+id/ivPillow"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/blanket"
                    style="@style/english"
                    android:text="Blanket" />

                <TextView
                    android:id="@+id/tvBlanket"
                    style="@style/translate"
                    android:text="Одеяло" />

                <ImageView
                    android:id="@+id/ivBlanket"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/sheet"
                    style="@style/english"
                    android:text="Sheet" />

                <TextView
                    android:id="@+id/tvSheet"
                    style="@style/translate"
                    android:text="Простынь" />

                <ImageView
                    android:id="@+id/ivSheet"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/duvetcover"
                    style="@style/english"
                    android:text="Duvet cover" />

                <TextView
                    android:id="@+id/tvDuvetcover"
                    style="@style/translate"
                    android:text="Пододеяльник" />

                <ImageView
                    android:id="@+id/ivDuvetcover"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/pillowcase"
                    style="@style/english"
                    android:text="Pillowcase" />

                <TextView
                    android:id="@+id/tvPillowcase"
                    style="@style/translate"
                    android:text="Наволочка" />

                <ImageView
                    android:id="@+id/ivPillowcase"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/curtains"
                    style="@style/english"
                    android:text="Curtains" />

                <TextView
                    android:id="@+id/tvCurtains"
                    style="@style/translate"
                    android:text="Шторы" />

                <ImageView
                    android:id="@+id/ivCurtains"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tulle"
                    style="@style/english"
                    android:text="Tulle" />

                <TextView
                    android:id="@+id/tvTulle"
                    style="@style/translate"
                    android:text="Тюль" />

                <ImageView
                    android:id="@+id/ivTulle"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/carpet"
                    style="@style/english"
                    android:text="Carpet" />

                <TextView
                    android:id="@+id/tvCarpet"
                    style="@style/translate"
                    android:text="Ковер" />

                <ImageView
                    android:id="@+id/ivCarpet"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/sofa"
                    style="@style/english"
                    android:text="Sofa" />

                <TextView
                    android:id="@+id/tvSofa"
                    style="@style/translate"
                    android:text="Диван" />

                <ImageView
                    android:id="@+id/ivSofa"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/lamp"
                    style="@style/english"
                    android:text="Lamp" />

                <TextView
                    android:id="@+id/tvLamp"
                    style="@style/translate"
                    android:text="Лампа" />

                <ImageView
                    android:id="@+id/ivLamp"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/mirror"
                    style="@style/english"
                    android:text="Mirror" />

                <TextView
                    android:id="@+id/tvMirror"
                    style="@style/translate"
                    android:text="Зеркало" />

                <ImageView
                    android:id="@+id/ivMirror"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/wardrobe"
                    style="@style/english"
                    android:text="Wardrobe" />

                <TextView
                    android:id="@+id/tvWardrobe"
                    style="@style/translate"
                    android:text="Гардеробная" />

                <ImageView
                    android:id="@+id/ivWardrobe"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>
<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/yellowfonone">

    <ImageView
        android:id="@+id/ivBackToNewWordsList"
        android:layout_width="78dp"
        android:layout_height="42dp"
        android:src="@drawable/leftarrow"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ScrollView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ivBackToNewWordsList"
        tools:ignore="SpeakableTextPresentCheck">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="8dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/city"
                    style="@style/english"
                    android:text="City" />

                <TextView
                    android:id="@+id/tvCity"
                    style="@style/translate"
                    android:text="Город" />

                <ImageView
                    android:id="@+id/ivCity"
                    style="@style/image_to_main"
                    android:layout_height="match_parent"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/street"
                    style="@style/english"
                    android:text="Street" />

                <TextView
                    android:id="@+id/tvStreet"
                    style="@style/translate"
                    android:text="Улица" />

                <ImageView
                    android:id="@+id/ivStreet"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/road"
                    style="@style/english"
                    android:text="Road" />

                <TextView
                    android:id="@+id/tvRoad"
                    style="@style/translate"
                    android:text="Дорога" />

                <ImageView
                    android:id="@+id/ivRoad"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/oneway"
                    style="@style/english"
                    android:text="Oneway" />

                <TextView
                    android:id="@+id/tvOneway"
                    style="@style/translate"
                    android:text="Одностороннее движение" />

                <ImageView
                    android:id="@+id/ivOneway"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/traffic"
                    style="@style/english"
                    android:text="Traffic" />

                <TextView
                    android:id="@+id/tvTraffic"
                    style="@style/translate"
                    android:text="Дорожное движение" />

                <ImageView
                    android:id="@+id/ivTraffic"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/busstop"
                    style="@style/english"
                    android:text="Bus stop" />

                <TextView
                    android:id="@+id/tvBusstop"
                    style="@style/translate"
                    android:text="Автобусная остановка" />

                <ImageView
                    android:id="@+id/ivBusstop"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/sidewalk"
                    style="@style/english"
                    android:text="Sidewalk" />

                <TextView
                    android:id="@+id/tvSidewalk"
                    style="@style/translate"
                    android:text="Тротуар" />

                <ImageView
                    android:id="@+id/ivSidewalk"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/border"
                    style="@style/english"
                    android:text="Border" />

                <TextView
                    android:id="@+id/tvBorder"
                    style="@style/translate"
                    android:text="Бордюр" />

                <ImageView
                    android:id="@+id/ivBorder"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/square"
                    style="@style/english"
                    android:text="Square" />

                <TextView
                    android:id="@+id/tvSquare"
                    style="@style/translate"
                    android:text="Площадь" />

                <ImageView
                    android:id="@+id/ivSquare"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/subway"
                    style="@style/english"
                    android:text="Subway" />

                <TextView
                    android:id="@+id/tvSubway"
                    style="@style/translate"
                    android:text="Метрополитен" />

                <ImageView
                    android:id="@+id/ivSubway"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/district"
                    style="@style/english"
                    android:text="District" />

                <TextView
                    android:id="@+id/tvDistrict"
                    style="@style/translate"
                    android:text="Район" />

                <ImageView
                    android:id="@+id/ivDistrict"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/quarter"
                    style="@style/english"
                    android:text="Quarter" />

                <TextView
                    android:id="@+id/tvQuarter"
                    style="@style/translate"
                    android:text="Квартал" />

                <ImageView
                    android:id="@+id/ivQuarter"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/pillar"
                    style="@style/english"
                    android:text="Pillar" />

                <TextView
                    android:id="@+id/tvPillar"
                    style="@style/translate"
                    android:text="Столб" />

                <ImageView
                    android:id="@+id/ivPillar"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/cemetery"
                    style="@style/english"
                    android:text="Cemetery" />

                <TextView
                    android:id="@+id/tvCemetery"
                    style="@style/translate"
                    android:text="Кладбище" />

                <ImageView
                    android:id="@+id/ivCemetery"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/broadway"
                    style="@style/english"
                    android:text="Broadway" />

                <TextView
                    android:id="@+id/tvBroadway"
                    style="@style/translate"
                    android:text="Бродвей" />

                <ImageView
                    android:id="@+id/ivBroadway"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/boulevard"
                    style="@style/english"
                    android:text="Boulevard" />

                <TextView
                    android:id="@+id/tvBoulevard"
                    style="@style/translate"
                    android:text="Бульвар" />

                <ImageView
                    android:id="@+id/ivBoulevard"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/avenue"
                    style="@style/english"
                    android:text="Avenue" />

                <TextView
                    android:id="@+id/tvAvenue"
                    style="@style/translate"
                    android:text="Проспект" />

                <ImageView
                    android:id="@+id/ivAvenue"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/embankment"
                    style="@style/english"
                    android:text="Embankment" />

                <TextView
                    android:id="@+id/tvEmbankment"
                    style="@style/translate"
                    android:text="Набережная" />

                <ImageView
                    android:id="@+id/ivEmbankment"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/bridge"
                    style="@style/english"
                    android:text="Bridge" />

                <TextView
                    android:id="@+id/tvBridge"
                    style="@style/translate"
                    android:text="Мост" />

                <ImageView
                    android:id="@+id/ivBridge"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/lawn"
                    style="@style/english"
                    android:text="Lawn" />

                <TextView
                    android:id="@+id/tvLawn"
                    style="@style/translate"
                    android:text="Газон" />

                <ImageView
                    android:id="@+id/ivLawn"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/park"
                    style="@style/english"
                    android:text="Park" />

                <TextView
                    android:id="@+id/tvPark"
                    style="@style/translate"
                    android:text="Парк" />

                <ImageView
                    android:id="@+id/ivPark"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/undergroundpass"
                    style="@style/english"
                    android:text="Underground pass" />

                <TextView
                    android:id="@+id/tvUndergroundpass"
                    style="@style/translate"
                    android:text="Подземный переход" />

                <ImageView
                    android:id="@+id/ivUndergroundpass"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/storeshop"
                    style="@style/english"
                    android:text="Store, Shop" />

                <TextView
                    android:id="@+id/tvStoreshope"
                    style="@style/translate"
                    android:text="Магазин" />

                <ImageView
                    android:id="@+id/ivStoreshop"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/market"
                    style="@style/english"
                    android:text="Market" />

                <TextView
                    android:id="@+id/tvMarket"
                    style="@style/translate"
                    android:text="Рынок" />

                <ImageView
                    android:id="@+id/ivMarket"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/ginger"
                    style="@style/english"
                    android:text="Ginger" />

                <TextView
                    android:id="@+id/tvGinger"
                    style="@style/translate"
                    android:text="Имбирь" />

                <ImageView
                    android:id="@+id/ivGinger"
                    style="@style/image_to_main"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_set_as" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="8dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/jerusalemartichoke"
